public class ArrayProgram
{
    public static void main(String args[])
    {
        //int[] arr =new int[3];
        int arr[] =new int[3];
        arr[0]=100;
        arr[1]=200;
        arr[2]=300;
        
        for(int i: arr)
        {
            System.out.println(i);
        }
    }
    
}
